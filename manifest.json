{
  "manifest_version": 2,

  "name": "Liquid-dl",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "0.2",
  "content_security_policy":"script-src 'self' http://10.181.56.192:8000/*; object-src 'self'",
  "browser_action": {
    "default_icon": "liq_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "http://*/",
    "http://10.181.56.192:8000/*",
    "http://192.168.56.1:8000/liquid-dl/youtubedl-submit"
  ]

}
